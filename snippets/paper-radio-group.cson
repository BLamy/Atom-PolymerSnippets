".text.html":
  "paper radio group":
    "prefix": "paper-radio-group"
    "body": """
    <paper-radio-group selected="${1:small}">
      <paper-radio-button name="${2:small}" label="${3:Small}"></paper-radio-button>
      <paper-radio-button name="${4:medium}" label="${5:Medium}"></paper-radio-button>
      <paper-radio-button name="${6:large}" label="${7:Large}"></paper-radio-button>
    </paper-radio-group>
    """
