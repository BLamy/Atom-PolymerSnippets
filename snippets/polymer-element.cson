".text.html":
  "polymer element":
    "prefix": "pe"
    "body": """
    ${1:<link rel="import" href="../polymer/polymer.html">}
    <polymer-element name="$2" attributes="$3">
    	<template>
    		<style>
    			:host {
    				display: block;
    			}
    		</style>$4
    	</template>
    	<script>
    		Polymer('$2', {

    		});
    	</script>
    </polymer-element>
    """
