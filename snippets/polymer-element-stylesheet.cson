".text.html":
  "polymer element, external styles":
    "prefix": "pes"
    "body": """
    <dom-module id="$1">
      <link rel="import" type="css" href="$1.css">
      <template>
        $2
      </template>
      <script>
        Polymer({
          is: '$1'
        });
      </script>
    </dom-module>
    """
